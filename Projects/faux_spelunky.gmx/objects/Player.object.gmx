            <kind>1</kind>
            <string>///Collide with enemy

var above_enemy = (y &lt; other.y + vspd);
var falling = vspd &gt; 0;//going down on the screen

                        //  ledge_grab part is for killing the bat if attacking from below...sneaky bastard.
if(above_enemy &amp;&amp; (falling || state == ledge_grab_state)){
    if(!place_meeting(x,yprevious,Solid)){//make sure not hitting the ceiling
        y = yprevious;
    }
    
    while(!place_meeting(x, y+1, other)){
        //exactly 1 px above enemy
        y++;
    }

    //kill the enemy
    with(other){
        instance_destroy();
    }
    
    //bounce up
    vspd = -(jspd/1.5);
    
    //Play step sound
    audio_play_sound(snd_step, 6, false);
    
}else{
    //Take damage
    take_damage();
}
</string>
          </argument>
        </arguments>
      </action>
